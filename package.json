{
  "name": "javascript-starterkit",
  "version": "1.0.0",
  "description": "Starter kit for starting a productive javascript application. Creating a \"pit of success\" so that stuff is built correctly without having to re-look up everything all the time.",
  "main": "index.js",
  "scripts": {
    "prestart": "babel-node scripts/serverMessage.js",
    "start": "npm-run-all --parallel security-check start:server",
    "start:server": "babel-node scripts/server.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "security-check": "nsp check",
    "localtunnel": "lt --port 3000",
    "share": "npm-run-all --parallel start:server localtunnel"
  },
  "dependencies": {
    "express": "^4.14.1",
    "open": "0.0.5",
    "path": "^0.12.7"
  },
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-loader": "^6.3.2",
    "chalk": "^1.1.3",
    "css-loader": "^0.26.2",
    "localtunnel": "^1.8.2",
    "npm-run-all": "^4.0.2",
    "nsp": "^2.6.2",
    "numeral": "^2.0.4",
    "style-loader": "^0.13.2",
    "webpack": "^2.2.1",
    "webpack-dev-middleware": "^1.10.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Cyberkruz/javascript-starterkit.git"
  },
  "keywords": [],
